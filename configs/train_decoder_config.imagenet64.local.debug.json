{
    "decoder": {
        "unets": [
            {
                "dim": 128,
                "image_embed_dim": 768,
                "cond_dim": 64,
                "channels": 3,
                "dim_mults": [1, 2, 4, 8],
                "attn_dim_head": 32,
                "attn_heads": 16,
		"self_attn": [false, true, true, true]
            }
        ],
        "image_sizes": [64],
        "channels": 3,
        "timesteps": 1000,
        "loss_type": "l2",
        "beta_schedule": ["cosine"],
        "learned_variance": true,
        "clip": {
            "make": "openai",
            "model": "ViT-L/14"
        }
    },
    "data": {
        "webdataset_base_url": "/nvme/xingzhening/data/imagenet/train_tar/{}.tar",
        "num_workers": 4,
        "batch_size": 64,
        "start_shard": 0,
        "end_shard": 256,
        "shard_width": 0,
        "index_width": 4,
        "splits": {
            "train": 0.75,
            "val": 0.15,
            "test": 0.1
        },
        "shuffle_train": true,
        "shuffle_num": 5000,
        "shuffle_init": true,
        "resample_train": false,
        "preprocessing": {
            "RandomResizedCrop": {
                "size": [224, 224],
                "scale": [0.75, 1.0],
                "ratio": [1.0, 1.0]
            },
            "ToTensor": true
        }
    },
    "train": {
        "epochs": 20,
        "lr": 1e-4,
        "wd": 0.01,
        "max_grad_norm": 0.5,
        "save_every_n_samples": 100000,
        "n_sample_images": 6,
        "device": "cuda:0",
        "epoch_samples": null,
        "validation_samples": null,
        "use_ema": true,
        "ema_beta": 0.99,
        "amp": false,
        "unet_training_mask": [true]
    },
    "evaluate": {
        "n_evaluation_samples": 100,
        "FID": {
            "feature": 2048
        },
        "IS": {
            "feature": 2048,
            "splits": 10
        },
        "KID": {
            "feature": 2048,
            "subset_size": 10
        },
        "LPIPS": {
            "net_type": "vgg",
            "reduction": "mean"
        }
    },
    "tracker": {
        "overwrite_data_path": true,

        "log": {
            "log_type": "console"
        },

        "load": {
            "load_from": null
        },

        "save": [{
            "save_to": "local",
            "save_latest_to": "imagenet-64-decoder/latest.pth"
        }]
    }
}
